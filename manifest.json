{
  "name": "AdvRemover",
  "description": "AdvRemover",
  "version": "1.0",
  "permissions": [
    "activeTab", "http://*/"
  ],
  
  "background": {
    "scripts": ["jquery-3.3.1.min.js", "background.js"],
	    "persistent": false
	  },
	  "browser_action": {
	    "default_title": "Remove all Ads"
	  },
	  "manifest_version": 2,
	  "icons": {
		  "16": "icon16.png",
		  "24": "icon24.png",
		  "32": "icon32.png",
		  "64": "icon64.png",
		  "128": "icon128.png"
		}
	}